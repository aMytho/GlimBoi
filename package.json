{
  "name": "glimboi",
  "productName": "GlimBoi",
  "version": "2.6.0",
  "description": "A chatbot for Glimesh.tv",
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aMytho/GlimBoi.git"
  },
  "build": {
    "appId": "GlimBoi",
    "asar": true,
    "asarUnpack": [
      "src/overlays/**",
      "resources/json/**"
    ],
    "win": {
      "icon": "resources/Icon/icon.png"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "resources/Icons/icon.ico",
      "installerHeaderIcon": "resources/Icons/icon.ico"
    },
    "linux": {
      "target": "deb",
      "synopsis": "A chatbot for Glimesh.tv",
      "maintainer": "Mytho",
      "vendor": "Mytho",
      "executableName": "GlimBoi",
      "icon": "resources/Icons/256x256.png"
    },
    "directories": {
      "buildResources": "resources/Icons"
    },
    "files": [
      "!docs/*",
      "!dist/*",
      "!@types/*",
      "!lib/*",
      "!scripts/*"
    ]
  },
  "scripts": {
    "build": "tsc --project tsconfig.main.json && ng build",
    "compile": "tsc --project tsconfig.main.json && ng build",
    "deploy": "node scripts/deploy.js",
    "dev": "tsc --watch",
    "start": "electron build/main.js & ng serve"
  },
  "author": "Mytho",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@seald-io/nedb": "^2.2.1",
    "angular-datatables": "^13.1.0",
    "crypto-js": "^4.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "electron-window-state": "^5.0.3",
    "esm": "^3.2.25",
    "flowbite": "^1.4.1",
    "jquery": "^3.6.0",
    "music-metadata": "^7.8.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "ws": "^7.5.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "~14.0.1",
    "@angular/compiler-cli": "^14.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/bootstrap": "^5.0.16",
    "@types/crypto-js": "^4.0.2",
    "@types/datatables.net": "^1.10.19",
    "@types/jasmine": "~4.0.0",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.17.4",
    "@types/ws": "^7.4.7",
    "autoprefixer": "^10.4.7",
    "electron": "^18.2.0",
    "electron-builder": "^22.11.11",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "open": "^8.4.0",
    "pkginfo": "^0.4.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.3.4"
  }
}
